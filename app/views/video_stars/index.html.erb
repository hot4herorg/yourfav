<p id="notice"><%= notice %></p>

<h1>Listing Video Stars</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @video_stars.each do |video_star| %>
      <tr>
        <td><%= link_to 'Show', video_star %></td>
        <td><%= link_to 'Edit', edit_video_star_path(video_star) %></td>
        <td><%= link_to 'Destroy', video_star, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video star', new_video_star_path %>
